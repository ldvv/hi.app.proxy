        `customerId`,
        `customerName`,
        `name`,
        `nickName`,
        `sex`,
        `country`,
        `stateProvince`,
        `city`,
        `language`,
        `currency`,
        `address`,
        `post`,
        `mobilePhone`,
        `telephone`,
        `email`,
        `facebook`,
        `twitter`,
        `googlePlus`,
        `birthday`,
