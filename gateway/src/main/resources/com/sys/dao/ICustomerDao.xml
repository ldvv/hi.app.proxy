<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sys.gateway.dao.ICustomerDao" >

    <insert id="updateToDB" parameterType="com.sys.gateway.vo.CustomerDB" >
